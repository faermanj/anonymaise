{
  "name": "Anonymaise Dev Container",
  "image": "mcr.microsoft.com/devcontainers/base",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {},
    "ghcr.io/audacioustux/devcontainers/graalvm:1": {
      "java": "25"
    },
    "ghcr.io/devcontainers/features/quarkus:1": {}
  },
  "postCreateCommand": "source ./devcontainer/postCreateCommand.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "aws-scripting-guy.cform",
        "vscjava.vscode-java-pack",
        "redhat.java"
      ]
    }
  }
}